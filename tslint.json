{
    "extends": ["tslint-react", "tslint-config-airbnb"],
    "rules":{
        "variable-name": false,
        "max-line-length": false,
        "no-boolean-literal-compare": false,
        "strict-boolean-expressions": false,
        "import-name": false,
        "indent": [true, "spaces", 4]
    },
    "indent": [true, "spaces", 4],
    "linterOptions": {
      "exclude": [
        "src/*.test.tsx"
      ]
  }
}
